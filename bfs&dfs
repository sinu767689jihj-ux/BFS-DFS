#include<stdio.h>
int a[20][20],visited[20],queue[20],n,f=0,r=0;
void BFS(int v)
{
    int i;
    for(i=0;i<n;i++)
    {
        if(a[v][i]!=0&&visited[i]==0)
        {
            r=r+1;
            queue[r]=i;
            visited[i]=1;
            printf(" %d",i);
        }
    }
    f=f+1;
    if(f<=r)
    BFS(queue[f]);
}
void DFS(int v)
{
    int i;
    for(i=0;i<n;i++)
    {
        if(a[v][i]!=0&&visited[i]==0)
        {
            visited[i]=1;
            printf(" %d",i);
            DFS(i);
        }
    }
}
// main function
int main()
{
    int i,j,v;
    printf("Enter the Number of vertex:");
    scanf("%d",&n);
    printf("Enter the Elements into graph Matrix:\n");
    for(i=0;i<n;i++)
    {
        for(j=0;j<n;j++)
        {
            scanf("%d",&a[i][j]);
        }
    }
    printf("Enter the starting value:");
    scanf("%d",&v);
    // BFS traversal 
    for(i=0;i<n;i++)
    visited[i]=0;
    f=0;
    r=0;
    queue[r]=v;
    visited[v]=1;
    printf("BFS Traversal is:\n");
    printf("%d",v);
    BFS(v);
    //DFS traversal
    for(i=0;i<n;i++)
    visited[i]=0;
    visited[v]=1;
    printf("\n DFS Traversal is:\n");
    printf("%d",v);
    DFS(v);
    printf("\n");
    return 0;
}
